{
  "name": "ts-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@7.33.6",
  "description": "typescript-template",
  "author": "peterroe",
  "license": "MIT",
  "keywords": [
    "typescript",
    "template"
  ],
  "homepage": "https://github.com/peterroe/un#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterroe/un.git"
  },
  "bugs": "https://github.com/peterroe/un/issues",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "release": "pnpm build bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "git-hook-init": "husky install && husky add .husky/pre-commit 'npm run lint && npm run typecheck'"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.35.3",
    "@types/node": "^18.17.18",
    "bumpp": "8.2.1",
    "eslint": "8.50.0",
    "esno": "0.16.3",
    "husky": "8.0.3",
    "taze": "^0.9.2",
    "tsup": "^6.7.0",
    "typescript": "4.9.5",
    "vite": "4.4.9",
    "vitest": "0.28.5"
  },
  "dependencies": {
    "consola": "^3.2.3"
  }
}
