{
  "name": "create-un",
  "version": "1.0.1",
  "packageManager": "pnpm@7.11.0",
  "description": "Create Universal template",
  "author": "peterroe",
  "license": "MIT",
  "homepage": "https://github.com/commandjs/create-un#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commandjs/create-un.git"
  },
  "bugs": "https://github.com/commandjs/create-un/issues",
  "keywords": [
    "cli",
    "typescript",
    "template"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "create-un": "./bin/index.mjs"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "release": "bumpp",
    "typecheck": "tsc --noEmit",
    "prepare": "git init & husky install"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.31.0",
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@types/node": "18.6.0",
    "@types/prompts": "^2.4.1",
    "@vitest/coverage-c8": "0.24.3",
    "bumpp": "8.2.1",
    "eslint": "8.23.1",
    "esno": "0.16.3",
    "fs-extra": "^10.1.0",
    "husky": "8.0.1",
    "kolorist": "^1.6.0",
    "minimist": "^1.2.7",
    "prompts": "^2.4.2",
    "typescript": "4.8.2",
    "unbuild": "0.8.11",
    "vite": "3.1.4",
    "vitest": "0.23.4"
  }
}
